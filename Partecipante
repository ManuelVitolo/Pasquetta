public class Partecipante {
    private String nome;
    private String cognome;
    private Partecipante partecipante[];
    private int i=0;
    
    public Partecipante(String Nome, String Cognome){
        setNome(Nome);
        setCognome(Cognome);
       }

public void setNome(String nome){
    this.nome=nome;
}

public final void setCognome(String cognome){
    this.cognome=cognome;
}

public String getNome(){
    return nome;
}

public String getCognome(){
    return cognome;
}

public Partecipante getPartecipante(int i){
    return partecipante[i];
}

public void setPartecipante(Partecipante partecipante){
    this.partecipante[i]=partecipante;
    i++;
}

}
   
